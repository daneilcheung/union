{
  "name": "@xo-union/union",
  "version": "0.0.1",
  "description": "",
  "main": "webpack.config.js",
  "config": {
    "process-config-file": "devprocesses.config.js",
    "local-docs-app-url": "http://localhost:8080"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "bootstrap.generators": "npm link ./packages/tools/generator-union",
    "build": "npm run build.patterns && npm run build.docs",
    "build.docs": "webpack --config webpack.docs.js",
    "build.patterns": "webpack --config webpack.patterns.js",
    "clean": "echo y | lerna clean && git clean -xf packages/pattern-library",
    "lint": "eslint --ext .js,.jsx,.json .",
    "logs": "pm2 logs --lines=40",
    "new.pattern": "yo union:pattern",
    "open": "open $npm_package_config_local_docs_app_url",
    "publish.packages": "lerna publish",
    "restart": "pm2 restart all",
    "start": "pm2 start $npm_package_config_process_config_file",
    "status": "pm2 status",
    "stop": "pm2 stop all",
    "test": "npm run test.patterns && npm run test.tools && npm run test.support && npm run lint",
    "test.patterns": "karma start --single-run",
    "test.patterns.chrome": "karma start --browsers=ChromeWithDevTools --single-run",
    "test.patterns.watch": "karma start",
    "test.support": "jasmine",
    "test.tools": "lerna run test --ignore '@xo-union/*'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "css-module-builder": "^1.0.0-beta.5",
    "eslint-plugin-json": "^1.2.0",
    "humanize-string": "latest",
    "jsx-to-string-2": "^1.0.0",
    "pascal-case": "latest",
    "prism-themes": "^1.0.0",
    "prismjs": "^1.6.0",
    "react": "^15.4.2",
    "react-docgen": "^2.9.1",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2"
  },
  "devDependencies": {
    "a-css-loader": "^1.0.0-beta.5",
    "babel": "^6.5.2",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "bootstrap": "4.0.0-alpha.6",
    "css": "^2.2.1",
    "css-byebye": "^1.0.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.15.0",
