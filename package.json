  "main": "webpack.config.js",
  "config": {
    "process-config-file": "devprocesses.config.js",
    "local-docs-app-url": "http://localhost:8080"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "bootstrap-generators": "npm link ./packages/tools/generator-union",
    "build": "npm run build-packages && webpack --config ./packages/docs/webpack.config.js",
    "build:experiment": "webpack --config ./webpack.experiment.js",
    "build:docs": "webpack --config ./pattern-library-docs/webpack.config.js",
    "build:packages": "webpack",
    "clean": "echo y | lerna clean && lerna exec -- rm -rf ./lib",
    "css": "gulp css",
    "gulp": "gulp",
    "logs": "pm2 logs --lines=40",
    "new:component": "yo union:component",
    "new:css-module": "yo union:css-module",
    "open": "open $npm_package_config_local_docs_app_url",
    "publish-packages": "lerna publish",
    "restart": "pm2 restart all",
    "start": "pm2 start $npm_package_config_process_config_file",
    "start:experiment": "webpack-dev-server --config ./pattern-library/docs/webpack.config.js",
    "status": "pm2 status",
    "stop": "pm2 stop all",
    "test": "karma start --single-run",
    "test:chrome": "karma start --browsers=ChromeWithDevTools --single-run",
    "test:watch": "karma start"
  },
