{
  "name": "@xo-union/union",
  "version": "0.0.1",
  "description": "",
  "main": "webpack.config.js",
  "engineStrict": true,
  "engines": {
    "node": ">=7.0.0 <8.0"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "bootstrap.generators": "npm link ./packages/tools/generator-union",
    "build": "npm run build.patterns && npm run build.docs",
    "build.docs": "webpack --config webpack.docs.js",
    "build.patterns": "webpack --config webpack.patterns.js",
    "clean": "npm run clean.dependencies && npm run clean.build",
    "clean.build": "git clean -xf packages/pattern-library",
    "clean.dependencies": "echo y | lerna clean",
    "lint": "eslint --ext .js,.jsx,.json .",
    "login": "sh ./scripts/login.sh",
    "logs": "pm2 logs --lines=40",
    "new.pattern": "yo union:pattern",
    "open": "open http://localhost:8080",
    "publish.assets": "sh ./scripts/publish-assets.sh",
    "publish.docs": "sh ./scripts/publish-docs.sh",
    "publish.packages": "sh ./scripts/publish-packages.sh",
    "restart": "pm2 restart all",
    "setup": "yarn install && npm run bootstrap && npm run build.patterns",
    "start": "pm2 start devprocesses.config.js",
    "status": "pm2 status",
    "stop": "pm2 stop all",
    "test": "yarn run test.patterns && yarn run test.tools && yarn run test.support && yarn run lint",
    "test.patterns": "karma start --single-run",
    "test.patterns.chrome": "karma start --browsers=ChromeWithDevTools --single-run",
    "test.patterns.watch": "karma start",
    "test.support": "jasmine",
    "test.tools": "lerna run test --ignore '@xo-union/*'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
